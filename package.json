{
  "name": "react-electors",
  "version": "2.0.0-0",
  "description": "A selector library using a React Hooks like API",
  "keywords": [],
  "homepage": "https://github.com/etienne-dldc/react-electors#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/react-electors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/react-electors.git"
  },
  "license": "ISC",
  "author": "Etienne Dldc",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/react-electors.esm.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint",
    "pub": "yarn build && np",
    "start": "tsdx watch",
    "test": "tsdx test --coverage",
    "test:watch": "tsdx test --watch",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.52",
    "husky": "^4.3.0",
    "np": "^6.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "dependencies": {
    "electors": "^2.0.0",
    "suub": "^3.1.0"
